{
  "name": "ai-agent",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "description": "一个基于 Vue 3 + Vite 开发的 AI 对话应用，采用道家风格的对话界面",
  "keywords": ["ai", "chat", "vue3", "vite", "typescript"],
  "author": "你的名字",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/你的用户名/ai-agent.git"
  },
  "bugs": {
    "url": "https://github.com/你的用户名/ai-agent/issues"
  },
  "homepage": "https://github.com/你的用户名/ai-agent#readme",
  "engines": {
    "bun": ">=1.0.0"
  },
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "bunx --bun vue-tsc && bunx --bun vite build",
    "preview": "bunx --bun vite preview",
    "type-check": "bunx --bun vue-tsc --build",
    "clean": "rm -rf dist node_modules .bun bun.lockb",
    "format": "bunx prettier --write .",
    "lint": "bunx eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix"
  },
  "dependencies": {
    "openai": "^4.78.1",
    "vue": "^3.4.15",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "typescript": "~5.6.3",
    "vite": "^6.0.5",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vue-tsc": "^2.1.10"
  }
}
