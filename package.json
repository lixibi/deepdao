{
  "name": "ai-agent",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "bun": ">=1.0.0"
  },
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "bunx --bun run-p type-check \"build-only {@}\" --",
    "preview": "bunx --bun vite preview",
    "build-only": "bunx --bun vite build",
    "type-check": "bunx --bun vue-tsc --build",
    "clean": "rm -rf dist node_modules .bun bun.lockb",
    "format": "bunx prettier --write .",
    "lint": "bunx eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix"
  },
  "dependencies": {
    "openai": "^4.78.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "npm-run-all2": "^7.0.2",
    "typescript": "~5.6.3",
    "vite": "^6.0.5",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vue-tsc": "^2.1.10"
  }
}
